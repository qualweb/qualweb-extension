    "@qualweb/locale": "0.1.10",
    "file-saver": "^2.0.2",
    "highlight.js": "^9.18.1",
    "vue": "^2.6.10",
    "vue-code-highlight": "^0.7.4",
    "vue-highlight.js": "^3.1.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "@types/chrome": "^0.0.74",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.1.1",
