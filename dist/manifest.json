{
  "name": "qualweb-extension",
  "description": "Accessibility evaluation using QualWeb",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.ico"
  },
  "permissions": [
    "tabs",
    {
      "fileSystem": [
        "write"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": [
    "icons/logoQW.png"
  ],
  "devtools_page": "devtools.html"
}